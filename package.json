{
  "name": "@singularit/multiselect",
  "version": "0.0.0",
  "private": false,
  "author": "singularIT GmbH",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/singularit-de/multiselect.git"
  },
  "bugs": {
    "url": "https://github.com/singularit-de/multiselect/issues"
  },
  "exports": {
    ".": {
      "require": "./dist/multiselect.umd.js",
      "import": "./dist/multiselect.mjs"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "dist/multiselect.umd.js",
  "module": "dist/multiselect.mjs",
  "types": "types",
  "files": [
    "dist",
    "types/**/*.d.ts",
    "*.md",
    "src",
    "!src/tests",
    "!src/stories"
  ],
  "scripts": {
    "dev": "vite",
    "webtypes": "vue-docgen-web-types --configFile web-types.config.js",
    "build": "vite build",
    "postbuild": "vue-tsc -p tsconfig.build-dts.json",
    "preview": "vite preview",
    "cypress:open": "cypress open --component",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "vue": "^3.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@cypress/vite-dev-server": "^5.0.7",
    "@cypress/vue": "^6.0.0",
    "@mdx-js/react": "^3.0.0",
    "@singularit/eslint-config-vue": "^1.2.0",
    "@storybook/addon-actions": "^7.6.7",
    "@storybook/addon-essentials": "^7.6.7",
    "@storybook/addon-interactions": "^7.6.7",
    "@storybook/addon-links": "^7.6.7",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/vue3": "^7.6.7",
    "@storybook/vue3-vite": "^7.6.7",
    "@types/lodash": "^4.14.182",
    "@vitejs/plugin-vue": "^5.0.2",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^9.1.3",
    "cypress": "^13.6.2",
    "eslint": "^8.22.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-vue": "^9.19.2",
    "gh-pages": "^4.0.0",
    "postcss": "^8.4.33",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.6.7",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.1.6",
    "vite": "^5.0.11",
    "vite-plugin-libcss": "^1.0.5",
    "vue-docgen-web-types": "^0.1.8",
    "vue-loader": "^17.4.2",
    "vue-tsc": "^1.8.27"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "beta",
        "prerelease": true
      }
    ],
    "npmPublish": true
  },
  "publishConfig": {
    "access": "public"
  },
  "web-types": "./web-types.json"
}
