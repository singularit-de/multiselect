{
  "name": "@singularit/multiselect",
  "private": false,
  "author": "singularIT GmbH",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/singularit-de/multiselect/issues"
  },
  "homepage": "https://singularit-de.github.io",
  "version": "0.0.0",
  "main": "dist/multiselect.umd.js",
  "module": "dist/multiselect.es.js",
  "exports": {
    ".": {
      "import": "./dist/multiselect.es.js",
      "require": "./dist/multiselect.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "cypress:open": "cypress open --component",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs",
    "deploy-storybook": "gh-pages -d storybook-static"
  },
  "files": [
    "dist",
    "*.md",
    "src",
    "!src/tests",
    "!src/stories"
  ],
  "dependencies": {
    "@cypress/vite-dev-server": "^2.2.3",
    "@cypress/vue": "^3.1.2",
    "cypress": "^10.0.0",
    "lodash": "^4.17.21",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@mdx-js/react": "^2.1.2",
    "@storybook/addon-actions": "^6.5.7",
    "@storybook/addon-essentials": "^6.5.7",
    "@storybook/addon-interactions": "^6.5.7",
    "@storybook/addon-links": "^6.5.7",
    "@storybook/builder-vite": "^0.1.36",
    "@storybook/testing-library": "0.0.11",
    "@storybook/vue3": "^6.5.7",
    "@types/lodash": "^4.14.182",
    "@vitejs/plugin-vue": "^2.3.1",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "gh-pages": "^4.0.0",
    "postcss": "^8.4.13",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.5.4",
    "vite": "^2.9.7",
    "vite-plugin-libcss": "^1.0.5",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^0.34.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/singularit-de/multiselect.git"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "develop",
        "prerelease": true
      }
    ],
    "npmPublish": true
  },
  "publishConfig": {
    "access": "public"
  }
}
